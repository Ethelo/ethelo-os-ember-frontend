<div class="container-fluid info-page" id={{page.componentId}}>
  {{bootstrap/bootstrap-modal-balance}}

  <!-- ROW ONE -->
  <div class="row special" id="row1">
    {{layout/left-column page=page}}

    <div id="center-col" class="col-xs-12 col-sm-7 col-md-6 col-md-offset-3 content-col blocked" role="main"aria-label="blocked-content-section">

      <div class="main">
        <h1 id="page-header" class="main">
          {{{t "navigation.page_unavailable_title"}}}
        </h1>
      </div>

      <div class="panel panel-default bootstrap-collapse" id="{{divId}}">

        <div class="collapse  in" id="blocked-collapse">
          <div class="panel-body">
            {{{t "navigation.page_unavailable_html"}}}
          </div>
        </div>
      </div>


      <div class="col-xs-12 col-sm-12 col-md-12">
        {{layout/bottom-nav-buttons hideNext=true}}
      </div>
    </div>

    <!-- RIGHT SIDEBAR -->

    <div class="col-xs-12 col-sm-5 col-md-3 right-sidebar fixed-right-column">

    </div>

  </div>
  <!-- END OF ROW ONE -->
</div>